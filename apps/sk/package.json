{
	"name": "@largeliarmodel/sveltekit-app",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "tsc -p server/tsconfig.json && vite build && npm run build:server",
		"build:server": "mkdir -p .svelte-kit/output/server && cp -r server/* .svelte-kit/output/server/",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio"
	},
	"devDependencies": {
		"@lucide/svelte": "^0.485.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.3.14",
		"clsx": "^2.1.1",
		"drizzle-kit": "^0.30.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-merge": "^3.0.2",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.0.0",
		"vite": "^6.0.0"
	},
	"dependencies": {
		"@biomejs/biome": "^2.0.0-beta.1",
		"@google/genai": "^0.7.0",
		"@google/generative-ai": "^0.24.0",
		"@libsql/client": "^0.15.1",
		"drizzle-orm": "^0.41.0",
		"hono": "^4.7.5",
		"zod": "^3.24.2"
	}
}
